{
  "name": "changeset-conventional-commits",
  "version": "0.2.5",
  "description": "Generate Changesets from conventional commits",
  "homepage": "http://github.com/iamchathu/changeset-conventional-commits",
  "main": "bin/index.js",
  "keywords": [
    "changesets",
    "changeset",
    "conventional commits"
  ],
  "files": [
    "/bin"
  ],
  "bin": {
    "changeset-conventional": "./bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iamchathu/changeset-conventional-commits.git"
  },
  "bugs": {
    "url": "https://github.com/iamchathu/changeset-conventional-commits/issues"
  },
  "author": {
    "name": "Chathu Vishawjith",
    "url": "https://chathu.me/"
  },
  "license": "MIT",
  "scripts": {
    "type-check": "tsc -p tsconfig.json",
    "build": "tsup",
    "test": "jest",
    "lint": "eslint 'src/**/*.{ts,tsx}' --quiet --fix",
    "ci:version": "changeset version && pnpm install --lockfile-only",
    "ci:publish": "changeset publish",
    "prepare": "husky install"
  },
  "dependencies": {
    "@changesets/read": "^0.6.0",
    "@changesets/types": "^6.0.0",
    "@changesets/write": "^0.3.1",
    "@manypkg/get-packages": "^2.2.2",
    "chalk": "^5.3.0",
    "figlet": "^1.7.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.6",
    "@jest/globals": "^29.7.0",
    "@swc/jest": "^0.2.36",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2"
  },
  "volta": {
    "node": "18.17.1"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
